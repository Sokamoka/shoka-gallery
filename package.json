{
  "name": "shoka-gallery",
  "version": "1.0.0-alpha.5",
  "description": "Super simple headless image gallery",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "Ákos Stégner",
    "email": "akos.stegner@gmail.com"
  },
  "scripts": {
    "build": "unbuild",
    "play": "pnpm run -C playground dev",
    "lint": "prettier -c --parser typescript \"{src,__tests__,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "pnpm run lint --write",
    "test:dev": "vitest",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "vitest --coverage",
    "test": "pnpm run lint && pnpm run test:types && pnpm run test:unit run"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "vue",
    "headless",
    "image",
    "gallery",
    "tailwindcss",
    "composition",
    "api"
  ],
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/test-utils": "2.0.0-rc.18",
    "happy-dom": "^8.1.1",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "unbuild": "^1.0.2",
    "vitest": "^0.26.3",
    "vue": "^3.2.45"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sokamoka/shoka-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/Sokamoka/shoka-gallery/issues"
  },
  "homepage": "https://github.com/Sokamoka/shoka-gallery#readme"
}
